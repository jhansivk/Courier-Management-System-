CREATE DATABASE Courier_Management;
USE Courier_Management;
create table User (UserID INT PRIMARY KEY,  
Name VARCHAR(255),  
Email VARCHAR(255) UNIQUE,  
 Password VARCHAR(255),  
ContactNumber VARCHAR(20),  
Address TEXT  );  
